    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         //Multiplier 
         eCoef is int
         eGain is int
         
         //Changes the cards
         gclPoker:Change()
         
         //The button is grayed
         MySelf..State = Grayed
         BTN_CHANGEALL..State = Grayed
         GR_PLACING..State = Active
         
         //Analyze the result
         eCoef = gclPoker:Analysis()
         
         //Depending on coefficient, display what was won:
         SWITCH eCoef 
         	CASE 0 : STC_EXPLANATION..Caption = <§$000d§> 
         	CASE 1 : STC_EXPLANATION..Caption = <§$0003§> 
         	CASE 2 : STC_EXPLANATION..Caption = <§$0004§> 
         	CASE 3 : STC_EXPLANATION..Caption = <§$0005§>
         	CASE 4 : STC_EXPLANATION..Caption = <§$000e§>  
         	CASE 5 : STC_EXPLANATION..Caption = <§$0006§> 
         	CASE 6 : STC_EXPLANATION..Caption = <§$0007§> 
         	CASE 7 : STC_EXPLANATION..Caption = <§$0008§> 
         	CASE 8 : STC_EXPLANATION..Caption = <§$0009§> 
         	CASE 10 : STC_EXPLANATION..Caption = <§$000a§>
         END
         
         //Multiply the gamble by coefficient and add it to REMAINDER
         eGain =  (EDT_PLACING*eCoef)
         EDT_REMAINDER = EDT_REMAINDER + eGain
         EDT_PLACING = 0
         
         EDT_GAIN += eGain
         
         STC_EXPLANATION..Caption = StringBuild(STC_EXPLANATION..Caption + <§$000b§>,eGain)
         
         //End of game
         IF EDT_REMAINDER = 0 THEN
         	IF YesNo(Yes,<§$000c§>) THEN
         		Initialize()
         	END
         	
         	// Subtracts $1 from remainder
         	IF Gamble(1) = True THEN
         		// Frees the game
         		BTN_PLAY..State = Active	
         	END
         	
         END
         
        type : 18
