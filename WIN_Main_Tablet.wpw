  procedures :
   -
     name : Gamble
     procedure_id : 1650959950873459185
     type_code : 14
     code : |1-
      PROCEDURE Gamble(_Val)
      
      // Subtracts $val from remainder
      EDT_REMAINDER -= _Val
      
      IF EDT_REMAINDER < 0 THEN 
      	//No money
      	Info(<§$0000§>)
      	EDT_REMAINDER += _Val
      	RESULT False
      ELSE
      	//Adds $1 to the gamble
      	EDT_PLACING += _Val
      END
      
      RESULT True
     type : 458752
   -
     name : Initialize
     procedure_id : 1650959950873524721
     type_code : 14
     code : |1+
      PROCEDURE Initialize()
      
      EDT_REMAINDER = cnInitialCreditAmount
      EDT_GAIN = 0
      EDT_PLACING = 0
      STC_EXPLANATION = <§$000f§>
      
      // The button is grayed
      BTN_PLAY..State = Grayed
      BTN_CHANGEALL..State = Grayed
      BTN_CHANGE..State = Grayed
      GR_PLACING..State = Active
      
      // Put all the cards in place
      IF gclPoker:ReturnAll() = True THEN
      	
      	ToastDisplay("Game reinitialized")
      END
      
     type : 458752
